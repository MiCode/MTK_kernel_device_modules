# MediaTek Clock Drivers
#
menu "Clock driver for MediaTek SoC"
	depends on ARM || ARM64 || COMPILE_TEST

config DEVICE_MODULES_COMMON_CLK_MEDIATEK
	tristate "Clock driver for MediaTek Socs"
	select RESET_CONTROLLER
	help
	  MediaTek SoCs' clock support.

config MTK_CLKMGR_FTRACE
	bool "Clock Manager ftrace Support for Mediatek SoCs"
	help
	  Choose this option if you have a Mediatek SoCs.
	  This config provide record MediaTek clk/pwr ftrace
	  to achieve better clk debug ability.
	  Set Yes to enable clk ftrace
	  Set No if not using MediaTek SoCs

config MTK_CLKMGR_DEBUG
	bool "Clock Manager Debug Support for Mediatek SoCs"
	help
	  Choose this option if you have a Mediatek SoCs.
	  This driver provides clock driver unit test nodes
	  and clock driver debug feature. Trigger by clock
	  debug cmd.

config COMMON_CLK_MT6761
	tristate "Clock driver for MediaTek MT6761"
	depends on ARM64 || ARM || COMPILE_TEST
	select DEVICE_MODULES_COMMON_CLK_MEDIATEK
	default MACH_MT6761
	help
	  This driver supports MediaTek MT6761 basic clocks,
	  which is used to control basic clock output.
	  If you wish to use clock resources from the MT6761 SOC,
	  say Y. Otherwise, say N.

