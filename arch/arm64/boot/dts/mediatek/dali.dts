/* nvt device tree node */
&pio {
	spi0_bus_active: spi0_bus_active {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO223__FUNC_SPI0_MI>, <PINMUX_GPIO222__FUNC_SPI0_MO>,
			<PINMUX_GPIO220__FUNC_SPI0_CLK>;
			slew-rate = <0>;
			drive-strength=<2>;
			bias-pull-down;
		};
	};

	spi0_cs_active: spi0_cs_active {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO221__FUNC_SPI0_CSB>;
			slew-rate = <0>;
			drive-strength=<2>;
			bias-pull-up;
		};
	};

	spi0_sleep: spi0_sleep {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO220__FUNC_GPIO220>, <PINMUX_GPIO221__FUNC_GPIO221>,
			<PINMUX_GPIO222__FUNC_GPIO222>, <PINMUX_GPIO223__FUNC_GPIO223>;
			slew-rate = <0>;
			bias-pull-down;
			input-enable;
		};
	};

	uart1_tx_active: uart1_tx_active {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO162__FUNC_UTXD1>;
			slew-rate = <0>;
			bias-disable;
		};
	};

	uart1_rx_active: uart1_rx_active {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO163__FUNC_URXD1>;
			slew-rate = <0>;
			bias-disable;
		};
	};

	uart1_sleep: uart1_sleep {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO162__FUNC_GPIO162>, <PINMUX_GPIO163__FUNC_GPIO163>;
			slew_rate = <0>;
			bias-pull-down;
			input-anable;
		};
	};
};

/* NVT ASIC Start */
&spi0 {
	status = "okay";
	dma-mode; /*enable spi dma mode */
	mediatek,pad-select = <0>;

	nvt {
		compatible = "novatek,vis-display";
		reg = <0x0>;
		status = "okay";
		spi-max-frequency = <26000000>;
		nvt,always08 = <&pio 26 0>;
		nvt,vsel08 = <&pio 207 0>;
		nvt,always18 = <&pio 181 0>;
		/*nvt,always12 = <&pio 9 0>;*/
		nvt,resetN = <&pio 186 0>;
		/*
		nvt,nonalways08 = <&pio 15 0>;
		nvt,nonalways18 = <&pio 17 0>;
		*/
		nvt,abnormal = <&pio 31 0>;
		nvt,ready = <&pio 193 0>;
		nvt,xtalEN = <&pio 206 0>;
		/*nvt,clock = <&pio 163 0>;*/
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&spi0_bus_active &spi0_cs_active &uart1_tx_active &uart1_rx_active>;
		pinctrl-1 = <&spi0_sleep &uart1_sleep>;
	};
};

&uart1 {
	status = "okay";
};
/* NVT ASIC End */



