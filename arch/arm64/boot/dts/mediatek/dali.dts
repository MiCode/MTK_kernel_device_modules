&chosen {
bootargs_ext = "cpufreq.default_governor=conservative \
		rcu_nocbs=0-7";
bootargs_ext_fac = "cpufreq.default_governor=conservative \
		rcu_nocbs=0-7";
};

&mtk_leds {
	compatible = "mediatek,disp-leds";

	backlight {
		max-brightness = <16383>;
		min-brightness = <1>;
		max-hw-brightness = <16383>;
		led_mode = <4>;
		default-state = "FifteenPercent";
	};
};

&afe {
	etdm-out-ch = <2 2 2 2 4 16 2>;
	etdm-in-ch = <2 2 2 2 4 16 16>;
	etdm-in-format = <0 0 0 0 4 0 0>; /*0: I2S; 1: LJ; 2: RJ; 3: EIAJ; 4: DSPA; 5: DSPB; 6: DSD */
	etdm-out-format = <0 0 0 0 4 0 0>;
	etdm-out-sync = <0 0 0 0 0 0 0>; /* 0: disable; 1: enable */
	etdm-in-sync = <1 0 0 0 1 1 1>; /* 0: disable; 1: enable */
	etdm-ip-mode = <0 0 0 0 0 0 0>; /* 0: One IP multi-channel 1: Multi-IP 2-channel */
};

&mt6373_adc {
	vin1_open {
		channel = <AUXADC_VIN1>;
		pures = <ADC_PURES_100K>;
	};
	vin2_open {
		channel = <AUXADC_VIN2>;
		pures = <ADC_PURES_100K>;
	};
	vin4_open {
