		#thermal-sensor-cells = <0>;

		/* reindex sensor if match sensor list */
		reindex-match = "imx766_mipi_raw",
				"imx709_mipi_raw",
				"imx866_mipi_raw";
		reindex-to = <0>;

		pinctrl-names = "mclk_off",
				"mclk_2mA",
				"mclk_4mA",
				"mclk_6mA",
				"mclk_8mA",
				"rst_low",
				"rst_high",
				"mclk1_off",
				"mclk1_2mA",
				"mclk1_4mA",
				"mclk1_6mA",
				"mclk1_8mA",
				"rst1_low",
				"rst1_high",
				"ponv_low",
				"ponv_high",
				"scl_ap",
				"scl_scp",
				"sda_ap",
				"sda_scp",
				"avdd1_off",
				"avdd1_on",
				"avdd3_off",
				"avdd3_on",
				"eint";
		pinctrl-0 = <&camera_pins_cam2_mclk_off>;
