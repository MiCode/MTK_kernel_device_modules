&mt6379_otg_vbus {
	regulator-max-microvolt = <7000000>;
};

&extcon_usb {
	vbus-voltage-max = <7000000>;
};

&i2c7 {
	clock-frequency = <400000>;
	sc96281@33{
		compatible = "sc,sc96281-wireless-charger";
		reg = <0x33>;
		fw_version_index_default = <0>;
		fw_version_index_jp = <1>;
		reverse_boost_src = <0>;
		revchg_boost_vol = <6800>;
		fwupdate_boost_vol = <5200>;
		q_value_supprot;
		tx_q1 = /bits/ 8 <0x0F 0x16>;
		tx_q2 = /bits/ 8 <0x20 0x20>;
		tx_q1_jp = /bits/ 8 <0x16 0x1B>;
		tx_q2_jp = /bits/ 8 <0x19 0x1B>;

		//support_hall = <0>;
		//rx_role = <0>;

		pwr_det_gpio = <&pio 208 0x00>;
		rx_irq_gpio = <&pio 4 0x00>;
		rx_sleep_gpio = <&pio 43 0x00>;
		pmic_vbus-supply = <&mt6379_otg_vbus>;
	};
};

&charger {
	wlschg_support;
	wls_revchg_support;
	wlschg_chip_vendor = <1>;
