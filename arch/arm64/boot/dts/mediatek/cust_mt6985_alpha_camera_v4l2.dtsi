&mt6373_vmch {
	regulator-compatible = "VMCH";
	regulator-name = "mt6373_vmch";
	regulator-min-microvolt = <3000000>;
	regulator-max-microvolt = <3000000>;
	regulator-enable-ramp-delay = <720>;
	regulator-allowed-modes = <0 2>;
	regulator-always-on;
};

