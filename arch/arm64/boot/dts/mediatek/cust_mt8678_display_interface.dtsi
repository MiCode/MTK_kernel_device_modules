&disp_dvo0 {
	status = "disabled";
	ports {
		port {
			disp_dvo0_out: endpoint {
				remote-endpoint = <&edptx_in>;
			};
		};
	};
};

&disp1_dp_intf0 {
	status = "disabled";

	ports {
		port@0 {
			reg = <0>;
			disp1_dp_intf0_out: endpoint {
				remote-endpoint = <&dptx_in0>;
			};
		};
	};
};

&disp1_dp_intf1 {
	status = "disabled";

	ports {
		port@0 {
			reg = <0>;
			disp1_dp_intf1_out: endpoint {
				remote-endpoint = <&dptx_in1>;
			};
		};
	};
};

&dp_tx {
	pinctrl-names = "default";
	pinctrl-0 = <&dp_gpio_def_cfg>;
	status = "disabled";
	direct-dp;

	ports {
		port@0 {
			reg = <0>;
			dptx_in0: endpoint {
				remote-endpoint = <&disp1_dp_intf0_out>;
			};
		};

		port@1 {
			reg = <1>;
			dptx_in1: endpoint {
				remote-endpoint = <&disp1_dp_intf1_out>;
			};
		};

		port@2 {
			reg = <2>;
			dptx_out0: endpoint {
				remote-endpoint = <&serdes_dp_in0>;
			};
		};
		port@3 {
			reg = <3>;
			dptx_out1: endpoint {
				remote-endpoint = <&serdes_dp_mst_in1>;
			};
		};
	};
};

&dsi0 {
	status = "disabled";
	#address-cells = <1>;
	#size-cells = <0>;
	panel1@0 {
		compatible = "lcm,dsi,max96789";
		reg = <0>;

		ser = <&max96789>;
		port {
			panel_in1: endpoint {
				remote-endpoint = <&dsi_out>;
			};
		};
	};
	panel2@1 {
		compatible = "boe,jd9365da";
		reg = <1>;
		power-gpios = <&pio 79 0>;
		reset-gpios = <&pio 90 0>;
		pinctrl-names = "default";
		port {
			panel_in2: endpoint {
				remote-endpoint = <&dsi_out2>;
			};
		};
	};
	ports {
		port {
			dsi_out: endpoint@0 {
				remote-endpoint = <&panel_in1>;
			};

			dsi_out2: endpoint@1 {
				remote-endpoint = <&panel_in2>;
			};
		};
	};
};

&dsi1 {
	status = "disabled";
