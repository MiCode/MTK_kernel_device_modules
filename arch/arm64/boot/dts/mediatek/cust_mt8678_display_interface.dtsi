&edp_tx {
	pinctrl-names = "default";
	pinctrl-0 = <&edp_gpio_def_cfg>;
	status = "disabled";
	max-lane-count = <4>;
	max-linkrate-mhz = <8100>;

	use-hpd = <1>;
	use-edid = <0>;
	external-monitor = <0>;

	ports {
		port@0 {
			reg = <0>;
			edptx_in: endpoint {
				remote-endpoint = <&disp_dvo0_out>;
			};
		};
		port@1 {
			reg = <1>;
			edptx_out: endpoint {
				remote-endpoint = <&serdes_edp_in>;
			};
		};
	};
};

